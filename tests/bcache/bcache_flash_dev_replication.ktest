#!/bin/bash

require-lib bcache-test-libs.sh

config-cache 2G,2G
config-bucket-size 512k
config-block-size 2k
config-volume 1400M
config-meta-replicas 2
config-data-replicas 2
config-mem 512M

config-timeout $(stress_timeout)

main()
{
    setup_tracing 64 'bcache:*'

    setup_bcache
    test_stress
    stop_bcache
}
