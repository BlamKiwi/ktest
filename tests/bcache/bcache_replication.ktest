#!/bin/bash

require-lib bcache-test-libs.sh

config-cache 256M,256M,256M
config-backing 2G
config-bucket-size 64k
config-block-size 4k
config-writeback
config-mem 512M
config-data-replicas 3
config-meta-replicas 3

config-timeout $(stress_timeout)

main()
{
    setup_tracing 'bcache:*'

    setup_bcache
    test_antagonist
    test_stress
    stop_bcache
}
