#!/bin/bash

require-lib bcache-test-libs.sh

config-cache 512M
config-backing 512M,512M
config-bucket-size 64k
config-block-size 4k

config-replacement random
config-writearound

config-timeout $(stress_timeout)

main()
{
    setup_tracing 'bcache:*'

    setup_bcache
    test_antagonist
    test_bcache_stres
    stop_bcache
}
